<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zhongxinxintuo.chuanjiabao">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WebViewActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".activity.LoginActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.RegisterSuccessActivity" />
        <activity android:name=".activity.RegisterPasswordActivity" />
        <activity android:name=".activity.LaunchActivity" />
        <activity android:name=".activity.FindPasswordActivity" />
        <activity android:name=".my.activity.MessageActivity" />
        <activity android:name=".my.activity.AccountActivity" />
        <activity android:name=".my.activity.MyOrderActivity"></activity>
    </application>

</manifest>